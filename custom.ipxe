#!ipxe
###
### netboot.xyz-custom menu
###

:start
echo Hello world!

:debian-10-cinnamon-squash
set ftp_url ftp://frontier-nas/docker/netbootxyz
goto boot

:boot
kernel ${ftp_url}/assets/Kerio-Control/boot/vmlinuz quiet initrd=${ftp_url}/assets/Kerio-Control/boot/initrd
#initrd ${ftp_url}/assets/Kerio-Control/boot/initrd
boot

goto custom_exit

:custom_exit
exit 0
